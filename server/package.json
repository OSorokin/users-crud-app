{
  "name": "users-crud-app-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migrations:generate-changelog": "node ./bin/Migrations.js --action=generate-changelog",
    "migrations:sync-test-scheme-with-models": "node ./bin/Migrations.js --action=sync-scheme-with-models",
    "migrations:apply-changelogs": "node ./bin/Migrations.js --action=apply-changelogs",
    "migrations:validate-scheme": "node ./bin/Migrations.js --action=validate-scheme",
    "migrations:apply-and-validate": "npm run migrations:apply-changelogs && npm run migrations:validate-scheme",
    "dev:compile": "npm install --only=dev && npm install --only=prod && gulp build:clean-and-compile",
    "dev:idea-debuggable-run": "node $NODE_DEBUG_OPTION ./bin/Application.js",
    "deployment:create-bundle": "gulp bundle:zip",
    "deployment:deploy": "gulp deploy",
    "dev:install-migrate-and-run": "npm install && npm run migrations:apply-and-validate && npm run dev:idea-debuggable-run",
    "predeployment:create-bundle": "gulp bundle:copy",
    "linux-only:migrations:generate-changelog": "mode=migrations npm run migrations:generate-changelog",
    "linux-only:migrations:sync-test-scheme-with-models": "mode=test npm run migrations:sync-test-scheme-with-models",
    "linux-only:start": "mode=local npm run start",
    "prestart": "npm install && npm run migrations:apply-and-validate",
    "start": "node ./bin/Application.js"
  },
  "dependencies": {
    "body-parser": "1.16.0",
    "class-transformer": "0.1.6",
    "class-validator": "0.6.4",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "2.6.0",
    "express": "4.14.1",
    "lodash": "4.16.6",
    "morgan": "1.7.0",
    "node-sass-middleware": "0.9.8",
    "pg": "6.1.2",
    "pg-hstore": "2.3.2",
    "pg-native": "1.10.0",
    "pg-parse-float": "0.0.1",
    "pg-promise": "5.6.2",
    "pgtools": "0.2.2",
    "routing-controllers": "0.6.6",
    "sequelize": "3.25.0",
    "serve-favicon": "2.3.2",
    "winston": "1.1.2",
    "winston-loggly-bulk": "1.3.4",
    "xlsx": "0.8.0",
    "xml2js": "0.4.17",
    "yargs": "6.3.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "0.0.42",
    "@types/bcrypt": "0.0.30",
    "@types/body-parser": "0.0.33",
    "@types/chai": "3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "1.3.30",
    "@types/cors": "0.0.33",
    "@types/express": "4.0.33",
    "@types/jade": "0.0.29",
    "@types/jsonwebtoken": "7.1.33",
    "@types/lodash": "4.14.38",
    "@types/mocha": "2.2.32",
    "@types/morgan": "1.7.32",
    "@types/node": "6.0.46",
    "@types/sequelize": "3.4.35",
    "@types/serve-favicon": "2.2.28",
    "@types/supertest-as-promised": "2.0.32",
    "@types/winston": "0.0.30",
    "@types/xlsx": "0.0.31",
    "@types/xml2js": "0.0.28",
    "@types/yargs": "6.3.2",
    "aws-sdk": "2.2.47",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "del": "2.2.0",
    "express-remove-route": "0.1.1",
