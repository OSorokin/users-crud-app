    "gulp": "3.9.1",
    "gulp-cached": "1.1.0",
    "gulp-install": "0.6.0",
    "gulp-mocha": "3.0.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "6.0.1",
    "gulp-typescript": "3.1.2",
    "gulp-util": "3.0.7",
